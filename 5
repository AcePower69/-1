#include <iostream>
using namespace std;
void main(){
    setlocale(LC_ALL,"Russian");
    double res,a,b,h;
    cout << "Введите начало: ";
    cin >> a;
    cout << "Введите конец: ";
    cin >> b;
    cout << "Введите шаг: ";
    cin >> h;
    if (a > b) {
        cout << "Не верный диапозон" << endl;
        exit;
    }
    for (double i = a; i <= b; i+=h){
        res = sqrt(5 - pow(i, 3));
        if (res>0)cout << res<< endl;
        else cout << "Не определена"<< endl;
        
    }
    system("pause");
};
